(roleId, permissionId) {\n    return request({\n      url: `${api_name}/doAssign`,\n      method: \"post\",\n      params: {roleId, permissionId}\n    })\n  }\n}\n"],"mappings":"AAAA,OAAOA,OAAO,MAAM,iBAAiB;;AAErC;AACA;AACA;AACA,IAAMC,QAAQ,GAAG,uBAAuB;AAExC,eAAe;EAEb;AACF;AACA;EACEC,iBAAiB,WAAAA,kBAAA,EAAG;IAClB,OAAOF,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,CAAE;MAClBI,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEC,gBAAgB,WAAAA,iBAACC,EAAE,EAAE;IACnB,OAAOP,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,cAAAG,MAAA,CAAWG,EAAE,CAAE;MAC/BF,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEG,aAAa,WAAAA,cAACC,UAAU,EAAE;IACxB,OAAOT,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,UAAO;MACvBI,MAAM,EAAE,MAAM;MACdK,IAAI,EAAED;IACR,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEE,gBAAgB,WAAAA,iBAACF,UAAU,EAAE;IAC3B,OAAOT,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,YAAS;MACzBI,MAAM,EAAE,KAAK;MACbK,IAAI,EAAED;IACR,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEG,QAAQ,WAAAA,SAACC,MAAM,EAAE;IACf,OAAOb,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,gBAAAG,MAAA,CAAaS,MAAM,CAAE;MACrCR,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACES,QAAQ,WAAAA,SAACD,MAAM,EAAEE,YAAY,EAAE;IAC7B,OAAOf,OAAO,CAAC;MACbG,GAAG,KAAAC,MAAA,CAAKH,QAAQ,cAAW;MAC3BI,MAAM,EAAE,MAAM;MACdW,MAAM,EAAE;QAACH,MAAM,EAANA,MAAM;QAAEE,YAAY,EAAZA;MAAY;IAC/B,CAAC,CAAC;EACJ;AACF,CAAC"}]}