 })\n    },\n\n    back() {\n      this.$router.push({ path: '/product/attrGroup/list' })\n    },\n\n    // 根据id查询记录\n    fetchDataById(id) {\n      api.getById(id).then(response => {\n        // debugger\n        this.attrGroup = response.data\n      })\n    }\n  }\n}\n</script>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;AAwBA,OAAAA,GAAA;AAEA,IAAAC,WAAA;EACAC,EAAA;EACAC,IAAA;EACAC,IAAA;EACAC,MAAA;AACA;AAEA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA,EAAAN,WAAA;MACAO,eAAA;IACA;EACA;EAEA;EACAC,KAAA;IACAC,MAAA,WAAAA,OAAAC,EAAA,EAAAC,IAAA;MACAC,OAAA,CAAAC,GAAA;MACAD,OAAA,CAAAC,GAAA,CAAAH,EAAA;MACAE,OAAA,CAAAC,GAAA,CAAAF,IAAA;MACA,KAAAG,IAAA;IACA;EACA;EAEA;EACAC,OAAA,WAAAA,QAAA;IACAH,OAAA,CAAAC,GAAA;IACA,KAAAC,IAAA;EACA;EAEAE,OAAA;IAEA;IACAF,IAAA,WAAAA,KAAA;MACA;MACA,SAAAL,MAAA,CAAAQ,MAAA,SAAAR,MAAA,CAAAQ,MAAA,CAAAhB,EAAA;QACA,IAAAA,EAAA,QAAAQ,MAAA,CAAAQ,MAAA,CAAAhB,EAAA;QACA,KAAAiB,aAAA,CAAAjB,EAAA;MACA;QACA;QACA,KAAAK,SAAA,GAAAa,aAAA,KAAAnB,WAAA;MACA;IACA;IAEAoB,YAAA,WAAAA,aAAA;MACA,KAAAb,eAAA;MACA,UAAAD,SAAA,CAAAL,EAAA;QACA,KAAAoB,QAAA;MACA;QACA,KAAAC,UAAA;MACA;IACA;IAEA;IACAD,QAAA,WAAAA,SAAA;MAAA,IAAAE,KAAA;MACAxB,GAAA,CAAAyB,IAAA,MAAAlB,SAAA,EAAAmB,IAAA,WAAAC,QAAA;QACA;QACA,IAAAA,QAAA,CAAAC,IAAA;UACAJ,KAAA,CAAAK,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAJ,QAAA,CAAAI;UACA;UACAP,KAAA,CAAAQ,OAAA,CAAAC,IAAA;YAAAC,IAAA;UAAA;QACA;MACA;IACA;IAEA;IACAX,UAAA,WAAAA,WAAA;MAAA,IAAAY,MAAA;MACAnC,GAAA,CAAAoC,UAAA,MAAA7B,SAAA,EAAAmB,IAAA,WAAAC,QAAA;QACA;QACA,IAAAA,QAAA,CAAAC,IAAA;UACAO,MAAA,CAAAN,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAJ,QAAA,CAAAI;UACA;UACAI,MAAA,CAAAH,OAAA,CAAAC,IAAA;YAAAC,IAAA;UAAA;QACA;MACA;IACA;IAEAG,IAAA,WAAAA,KAAA;MACA,KAAAL,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IAEA;IACAf,aAAA,WAAAA,cAAAjB,EAAA;MAAA,IAAAoC,MAAA;MACAtC,GAAA,CAAAuC,OAAA,CAAArC,EAAA,EAAAwB,IAAA,WAAAC,QAAA;QACA;QACAW,MAAA,CAAA/B,SAAA,GAAAoB,QAAA,CAAArB,IAAA;MACA;IACA;EACA;AACA"}]}