 {\n    return request({\n      url: `${api_name}/remove/${id}`,\n      method: 'delete'\n    })\n  },\n\n  /* \n  批量删除多个角色\n  */\n  removeRoles(ids) {\n    return request({\n      url: `${api_name}/batchRemove`,\n      method: 'delete',\n      data: ids\n    })\n  }\n}\n"],"mappings":";AAAA;AACA;AACA;AACA,OAAOA,OAAO,MAAM,iBAAiB;AAErC,IAAMC,QAAQ,GAAG,iBAAiB;AAElC,eAAe;EAEb;AACF;AACA;EACEC,WAAW,WAAAA,YAACC,IAAI,EAAEC,KAAK,EAAEC,SAAS,EAAE;IAClC,OAAOL,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,OAAAM,MAAA,CAAIJ,IAAI,OAAAI,MAAA,CAAIH,KAAK,CAAE;MACnCI,MAAM,EAAE,KAAK;MACbC,MAAM,EAAEJ,SAAS,CAAC;IACpB,CAAC,CAAC;EACJ,CAAC;EAEC;AACJ;AACA;EACEK,IAAI,WAAAA,KAACC,IAAI,EAAE;IACT,OAAOX,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,UAAO;MACvBO,MAAM,EAAE,MAAM;MACdI,IAAI,EAAED;IACR,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEE,OAAO,WAAAA,QAACC,EAAE,EAAE;IACV,OAAOd,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,WAAAM,MAAA,CAAQO,EAAE,CAAE;MAC5BN,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAID;AACF;AACA;EACEO,UAAU,WAAAA,WAACJ,IAAI,EAAE;IACf,OAAOX,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,YAAS;MACzBO,MAAM,EAAE,KAAK;MACbI,IAAI,EAAED;IACR,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEK,SAAS,WAAAA,UAACC,MAAM,EAAE;IAChB,OAAOjB,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,gBAAAM,MAAA,CAAaU,MAAM,CAAE;MACrCT,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEU,UAAU,WAAAA,WAACJ,EAAE,EAAE;IACb,OAAOd,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,cAAAM,MAAA,CAAWO,EAAE,CAAE;MAC/BN,MAAM,EAAE;IACV,CAAC,CAAC;EACJ,CAAC;EAED;AACF;AACA;EACEW,WAAW,WAAAA,YAACC,GAAG,EAAE;IACf,OAAOpB,OAAO,CAAC;MACbM,GAAG,KAAAC,MAAA,CAAKN,QAAQ,iBAAc;MAC9BO,MAAM,EAAE,QAAQ;MAChBI,IAAI,EAAEQ;IACR,CAAC,CAAC;EACJ;AACF,CAAC"}]}